<MudAppBar Class="pb-3" Elevation="1" Style="background: #f5f7fa;">
	<MudText Typo="Typo.h5" Style="color: black;">Welcome back Dr John!</MudText>
	<MudSpacer />

	<MudTextField Placeholder="@("Search for anything...")" Adornment="Adornment.End" AdornmentIcon="@Icons.Material.Outlined.Search" Style="border-radius: 20px; width: 80%; background: white;" Variant="Variant.Outlined" T="string" />
	<MudFab Class="ml-3 mr-3" Size="Size.Small" Style="background: white; color: #0A0A6A;" StartIcon="@Icons.Material.Outlined.Notifications" />

	<MudStack Style="background: white; padding: 10px; border-radius: 20px;" Row>
		<MudAvatar>
			<MudImage Src="profilePicture.png"></MudImage>
		</MudAvatar>

		<MudStack Spacing="0">
			<MudText Typo="Typo.body1" Style="color: black;">John Doe</MudText>
			<MudText Typo="Typo.body2" Style="color: darkgray;">Researcher Rx Ltd</MudText>
		</MudStack>

		<MudIcon Style="color: black;" Icon="@Icons.Material.Outlined.KeyboardArrowDown" />
	</MudStack>
</MudAppBar>